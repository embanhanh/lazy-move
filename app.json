{
  "expo": {
    "name": "Lazy Move",
    "slug": "lazy-move",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.embanhanh.lazymove",
      "supportsTablet": true
    },
    "android": {
      "package": "com.embanhanh.lazymove",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": "immersive-sticky",
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "permissions": ["SYSTEM_ALERT_WINDOW", "WRITE_SETTINGS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "hidden": true,
      "translucent": true,
      "barStyle": "light-content",
      "backgroundColor": "#00000000"
    },
    "androidNavigationBar": {
      "visible": "immersive-sticky",
      "backgroundColor": "#00000000",
      "barStyle": "light-content"
    },
    "extra": {
      "eas": {
        "projectId": "e3de120d-aa55-4fd9-88e3-e9231301feff"
      }
    },
    "plugins": ["expo-dev-client"],
    "fonts": {
      "Quicksand-Regular": "./assets/fonts/Quicksand-Regular.ttf",
      "Quicksand-Medium": "./assets/fonts/Quicksand-Medium.ttf",
      "Quicksand-SemiBold": "./assets/fonts/Quicksand-SemiBold.ttf",
      "Quicksand-Bold": "./assets/fonts/Quicksand-Bold.ttf"
    }
  }
}
